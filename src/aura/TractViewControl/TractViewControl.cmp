<aura:component implements="flexipage:availableForAllPageTypes">
	<div class="slds-page-header" style="border-bottom: 1px solid #dddbda; box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.1);">
        <div class="slds-media">
            <div class="slds-media__figure">
                <lightning:icon iconName="utility:search" size="normal" alternativeText="Search icon"/>
            </div>
            <div class="slds-media__body">
                <h1 class="slds-page-header__title slds-truncate slds-align-middle">Find a Tract</h1>
                <p class="slds-text-body_small slds-line-height_reset">Enter any part of a tract name, separated by spaces.  For example: "Howard T2N 33"</p>
                <p>
                    <c:TractLookupContainer/>
                </p>
            </div>
        </div>
    </div>
</aura:component>