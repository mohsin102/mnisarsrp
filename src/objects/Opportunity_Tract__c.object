        <fullName>Rollup_Exception__c</fullName>
        <externalId>false</externalId>
        <formula>Tract_Acres__c != Tract__r.netAcres__c || 
OTG_Cost_Per_Acre__c != Tract__r.OTG_Cost_Per_Acre__c ||
FMV_Per_Acre__c != Tract__r.Value_Per_Acre__c ||
ROUND(Total_Cost__c,2) != ROUND(Tract__r.Total_Cost__c,2) ||
ROUND(Total_Cost_Per_Acre__c,2) != ROUND(Tract__r.Total_Cost_Per_Acre__c,2)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
