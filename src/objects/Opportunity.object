    <validationRules>
        <fullName>Acquisition_Buyer_Required</fullName>
        <active>true</active>
        <description>Buyer must be set for an Acquisition</description>
        <errorConditionFormula>ISPICKVAL(Type,&apos;Acquisition&apos;) &amp;&amp; ISBLANK(Acquiring_Entity__c)</errorConditionFormula>
        <errorMessage>Acquiring Entity must be set for an Acquisition Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Divestiture_Seller_Required</fullName>
        <active>true</active>
        <description>Seller must be set for a Divestiture</description>
        <errorConditionFormula>ISPICKVAL(Type,&apos;Divestiture&apos;) &amp;&amp; ISBLANK(AccountId)</errorConditionFormula>
        <errorMessage>Seller (&quot;Account&quot; field) must be set for an Divestiture</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MandateEffectiveDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL(StageName,&apos;Closed Won&apos;), ISPICKVAL(Buyer_Status__c, &apos;100 - Closed&apos;)), ISBLANK( Effective_Date__c ))</errorConditionFormula>
        <errorMessage>Effective date is mandatory for Closed Won stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_fields_for_Closed</fullName>
        <active>true</active>
        <description>If it&apos;s closed won, enforce specific fields</description>
        <errorConditionFormula>(ISPICKVAL(StageName,&apos;Closed Won&apos;)) &amp;&amp; (ISBLANK(CloseDate) || ISBLANK(Acquiring_Entity__c))</errorConditionFormula>
        <errorMessage>In order to close, an Acquiring Entity and Close Date must be set</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_must_match_Record_Type</fullName>
        <active>false</active>
        <errorConditionFormula>Text(Type) != RecordType.Name</errorConditionFormula>
        <errorDisplayField>Type</errorDisplayField>
        <errorMessage>Opportunity Type (Acquisition, Divestiture) must match Record Type</errorMessage>
    </validationRules>
