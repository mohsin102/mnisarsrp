    <fields>
        <fullName>opportunityTypeCalculated__c</fullName>
        <externalId>false</externalId>
        <formula>if( TEXT(Account.Type) == &apos;Portfolio Investment&apos; &amp;&amp;  TEXT(Acquiring_Entity__r.Type) == &apos;Portfolio Investment&apos;,
    &apos;Inter-Company&apos;,
