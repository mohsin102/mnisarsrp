    <validationRules>
        <fullName>EitherPhoneOrAddress</fullName>
        <active>true</active>
        <description>User can Either enter Phone Or Address fields</description>
        <errorConditionFormula>AND( NOT(ISBLANK(Phone__c)),OR( NOT(ISBLANK(Street__c)), NOT(ISBLANK(City__c)), NOT(ISBLANK(stateProvince__c)), NOT(ISBLANK(zipPostalCode__c)), NOT(ISBLANK(Country__c))))</errorConditionFormula>
        <errorMessage>Please enter Phone or Address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PhoneOrAddressMandatory</fullName>
        <active>true</active>
        <description>Phone Or Address fields is mandatory</description>
        <errorConditionFormula>AND( ISBLANK(Phone__c),ISBLANK(Street__c), ISBLANK(City__c), ISBLANK(stateProvince__c), ISBLANK(zipPostalCode__c), ISBLANK(Country__c))</errorConditionFormula>
        <errorMessage>Please enter Phone or Address as contact Information</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Phone</fullName>
        <active>true</active>
        <description>Phone number should be of 10 digits</description>
        <errorConditionFormula>AND( NOT(ISBLANK(Phone__c)) ,LEN(Phone__c) != 10 )</errorConditionFormula>
        <errorMessage>Please enter a valid phone number</errorMessage>
    </validationRules>
