    <fields>
        <fullName>expirationDate__c</fullName>
        <externalId>false</externalId>
        <formula>IF( !ISBLANK(Effective_Date__c), 
  IF( extended__c , 
    /*All necessary fields are populated*/
    IF(!ISBLANK( extensionTerm__c ) &amp;&amp; !ISBLANK(TEXT( extensionTermMeasure__c ))&amp;&amp;!ISBLANK( term__c )&amp;&amp;!ISBLANK(TEXT( termMeasure__c )), 
      IF( TEXT(termMeasure__c) == &quot;Year&quot; &amp;&amp;  TEXT(extensionTermMeasure__c) ==&quot;Year&quot;, 
        ADDMONTHS(ADDMONTHS( Effective_Date__c , term__c * 12), extensionTerm__c * 12)  , 
        IF(TEXT(termMeasure__c) == &quot;Year&quot; &amp;&amp;  TEXT(extensionTermMeasure__c) ==&quot;Month&quot;, 
          ADDMONTHS(ADDMONTHS( Effective_Date__c , term__c * 12), extensionTerm__c), 
          IF(TEXT(termMeasure__c) == &quot;Month&quot; &amp;&amp;  TEXT(extensionTermMeasure__c) ==&quot;Month&quot;, 
            ADDMONTHS(ADDMONTHS( Effective_Date__c , term__c ), extensionTerm__c), 
            IF(TEXT(termMeasure__c) == &quot;Month&quot; &amp;&amp;  TEXT(extensionTermMeasure__c) ==&quot;Year&quot;, 
              ADDMONTHS(ADDMONTHS( Effective_Date__c , term__c ), extensionTerm__c * 12), 
               null ) ) ) ) , 
    /*if extended, validation rule requires extension fields to be populated, but need to check initial term and option*/
     IF(ISBLANK(term__c ) || ISBLANK(TEXT(termMeasure__c )), 
        IF(TEXT(extensionTermMeasure__c) ==&quot;Year&quot;, 
          ADDMONTHS( Effective_Date__c , extensionTerm__c * 12), 
          IF(TEXT(extensionTermMeasure__c) ==&quot;Month&quot;, 
            ADDMONTHS( Effective_Date__c , extensionTerm__c), 
            null) ) , 
        null ) ) ,
  /*Not extended */
  IF(!ISBLANK( term__c )&amp;&amp;!ISBLANK(TEXT( termMeasure__c )),
     IF(TEXT(termMeasure__c) == &quot;Year&quot;, 
       ADDMONTHS( Effective_Date__c , term__c * 12), 
        IF(TEXT(termMeasure__c) == &quot;Month&quot;, 
          ADDMONTHS( Effective_Date__c , term__c), 
          null) ) , 
    /*No valid options, use Effective Date*/
    null) ) , 
  null)</formula>
        <label>Expiration Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
