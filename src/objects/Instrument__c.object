    <validationRules>
        <fullName>Require_Extension_Term_and_Term_Measure</fullName>
        <active>true</active>
        <description>If either is populated, the other field must be.</description>
        <errorConditionFormula>(!ISBLANK( extensionTerm__c )&amp;&amp; ISBLANK(TEXT( extensionTermMeasure__c )))||
(ISBLANK(extensionTerm__c )&amp;&amp; !ISBLANK(TEXT(extensionTermMeasure__c )))</errorConditionFormula>
        <errorMessage>For a valid Extension Term you must enter both Extension Term and Extension Term Measure.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Extension_Values_when_Extended</fullName>
        <active>true</active>
        <description>Require extension option and extension measure are populated when setting extended to true.</description>
        <errorConditionFormula>extended__c &amp;&amp; (ISBLANK( extensionTerm__c ) || ISBLANK(TEXT( extensionTermMeasure__c )))</errorConditionFormula>
        <errorMessage>You must enter an Extension Option and Extension Option Term Measure value when setting Extended to true.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Term_and_Term_Measure</fullName>
        <active>true</active>
        <description>If either is populated, the other field must be.</description>
        <errorConditionFormula>(!ISBLANK(term__c  )&amp;&amp; ISBLANK(TEXT(termMeasure__c )))||
(ISBLANK(term__c  )&amp;&amp; !ISBLANK(TEXT(termMeasure__c )))</errorConditionFormula>
        <errorMessage>For a valid Term you must enter both Term and Term Measure.</errorMessage>
    </validationRules>
