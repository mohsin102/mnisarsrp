    <fields>
        <fullName>Month__c</fullName>
        <externalId>false</externalId>
        <label>Month</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Price_Net__c</fullName>
        <description>Multiplies price by the metadata net adjustment value</description>
        <externalId>false</externalId>
        <formula>CASE( TEXT(Pricing_Type__c),&apos;GAS&apos;,
$CustomMetadata.Production_Fee__mdt.Gas.Value__c,
$CustomMetadata.Production_Fee__mdt.Oil.Value__c)
*  Price__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Multiplies price by the metadata net adjustment value</inlineHelpText>
        <label>Price (Net)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Price__c</fullName>
        <description>Avg price</description>
        <externalId>false</externalId>
        <inlineHelpText>Avg price</inlineHelpText>
        <label>Price</label>
        <precision>18</precision>
        <required>true</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Pricing_Type__c</fullName>
        <externalId>false</externalId>
        <label>Pricing Type</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>OIL</fullName>
                    <default>false</default>
                    <label>OIL</label>
                </value>
                <value>
                    <fullName>GAS</fullName>
                    <default>false</default>
                    <label>GAS</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <label>Monthly Pricing</label>
    <listViews>
        <fullName>All</fullName>
